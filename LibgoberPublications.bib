keywords = {Bureaucratic politics,Data sources,Interest groups,Lobbying,Rulemaking},
number = {3},
pages = {425--435},
title = {{Data and Methods for Analyzing Special Interest Influence in Rulemaking}},
volume = {9},
year = {2020}
}
