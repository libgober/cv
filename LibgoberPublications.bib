@article{Libgober2020,
abstract = {{\textcopyright} 2020 by the Southern Political Science Association. All rights reserved. Agencies use notice-and-comment rulemaking to issue countless regulations with substantial economic stakes. The empirical literature on rulemaking has produced a complex set of descriptive findings yet has struggled with informal concerns about selection bias. This article characterizes notice and comment as a persuasion game played between regulators and outside interests. Analysis of this stakeholder-balancing model yields three key theoretical payoffs: an informational rationale for regulators to write rules with higher private and social costs, an explanation for strategic positioning by regulators even without oversight, and clarification that adverse priors are a more powerful mobilizing force than adverse policies. The model's two-sided selection dynamics reveal that well-established empirical regularities are inconsistent with extreme public-interest zealotry and strong capture but fit a range of intermediate outcomes. To obtain deeper insights about bias in rulemaking, the model suggests focusing on the cost of rule revision, rule movement following abstention, and variation in stakeholder preferences.},
author = {Libgober, Brian},
doi = {10.1086/706891},
file = {:Users/brianlibgober/Documents/Mendeley Desktop/Libgober - 2020 - Strategic Proposals, Endogenous Comments, and Bias in Rulemaking.pdf:pdf},
issn = {0022-3816},
journal = {The Journal of Politics},
month = {apr},
number = {2},
pages = {642--656},
title = {{Strategic Proposals, Endogenous Comments, and Bias in Rulemaking}},
url = {https://www.journals.uchicago.edu/doi/10.1086/706891},
volume = {82},
year = {2020}
}
